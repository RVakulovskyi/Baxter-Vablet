    <validationRules>
        <fullName>BST_Global_Record_Type</fullName>
        <active>true</active>
        <description>This validation makes sure that  Question type and profile type fields are populated if the record type is Global Question</description>
        <errorConditionFormula>AND (RecordType.DeveloperName  = &quot;BST_Global_Question&quot;, OR( ISBLANK(TEXT(BST_QUESTION_TYPE__c)) || ISBLANK(TEXT(BST_PROFILE_TYPE__c))))</errorConditionFormula>
        <errorMessage>Question Type and Profile Type are mandatory for Record Type : Global Question</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BST_Response_Valid_Days</fullName>
        <active>false</active>
        <description>This validation makes sure that the response Valid date should never be blank while saving a record.</description>
        <errorConditionFormula>ISBLANK(BST_RESPONSE_VALID_TIME__c)</errorConditionFormula>
        <errorMessage>The Response Valid Time(In days) field can not be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BST_Start_And_End_Date_Criteria</fullName>
        <active>true</active>
        <description>This is added to make sure that the Start Date is not past the End Date</description>
        <errorConditionFormula>BST_START_DATE__c  &gt;  BST_END_DATE__c</errorConditionFormula>
        <errorDisplayField>BST_START_DATE__c</errorDisplayField>
        <errorMessage>Please correct the errors.The Start Date can not be greater than End Date .</errorMessage>
    </validationRules>
