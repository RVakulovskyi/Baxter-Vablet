    <validationRules>
        <fullName>BST_Charte_Medical_Only_for_France</fullName>
        <active>true</active>
        <description>Validating that &quot;Charte Medical&quot; can only be enabled for French users</description>
        <errorConditionFormula>AND(  TEXT(BST_COUNTRY__c) &lt;&gt;&quot;France&quot;, BST_CHARTE_MEDICAL__c =TRUE)</errorConditionFormula>
        <errorDisplayField>BST_COUNTRY__c</errorDisplayField>
        <errorMessage>Only French users can be under &quot;Charte Medical&quot; process. Please make sure this is enabled only for the correct users in France</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BST_Country_Is_Required</fullName>
        <active>true</active>
        <description>For BOOST Users Country field is required.</description>
        <errorConditionFormula>AND(  
	BEGINS( Profile.Name , &apos;BOOST&apos;)  , 
	ISBLANK( TEXT(BST_COUNTRY__c) )
)</errorConditionFormula>
        <errorDisplayField>BST_COUNTRY__c</errorDisplayField>
        <errorMessage>Country is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BST_Main_Baxter_Team_Is_Required</fullName>
        <active>true</active>
        <description>For BOOST Users Main Baxter Team is required.</description>
        <errorConditionFormula>AND(  
	BEGINS( Profile.Name , &apos;BOOST&apos;)  , 
	ISBLANK( TEXT(BST_MAIN_BAXTER_TEAM__c) )
)</errorConditionFormula>
        <errorDisplayField>BST_MAIN_BAXTER_TEAM__c</errorDisplayField>
        <errorMessage>Main Baxter Team is required.</errorMessage>
    </validationRules>
